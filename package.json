{
  "name": "backoffice-api",
  "version": "0.0.1",
  "description": "backoffice - Thummy",
  "author": "Alisson Patrick Louren√ßo",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --webpack",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "env-cmd -f .env nest start --debug 0.0.0.0:9229 --watch",
    "start:debug": "env-cmd -f .env nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:sls": "yarn run build && sls offline start",
    "deploy:dev": "yarn run build && sls deploy --stage dev -r us-east-1 -c serverless-dev.yml",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "doc": "yarn compodoc -p tsconfig.json",
    "pretypeorm": "ts-node -r tsconfig-paths/register scripts/write-type-orm-config.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "yarn typeorm -- migration:generate -n",
    "typeorm:migration:run": "yarn typeorm -- migration:run",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test --passWithNoTests"
    }
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "^1.1.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/swagger": "^5.2.0",
    "@types/aws-lambda": "^8.10.85",
    "@types/aws-serverless-express": "^3.3.5",
    "@vendia/serverless-express": "^4.5.2",
    "aws-lambda": "^1.0.7",
    "aws-serverless-express": "^3.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "date-fns": "^2.16.1",
    "dotenv": "^16.0.0",
    "nest-router": "^1.0.9",
    "nodemailer": "^6.7.2",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.4",
    "rxjs-compat": "^6.6.7",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.6",
    "@nestjs/testing": "^8.2.6",
    "@types/aws-sdk": "^2.7.0",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.8",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.17",
    "@types/rx": "^4.1.2",
    "@types/supertest": "^2.0.10",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "aws-sdk": "^2.1062.0",
    "cache-loader": "^4.1.0",
    "corejs": "^1.0.0",
    "dotenv-webpack": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.3",
    "prettier": "^2.1.2",
    "serverless-bundle": "^5.2.0",
    "serverless-layers": "^2.5.2",
    "serverless-offline": "^8.3.0",
    "serverless-plugin-warmup": "^7.0.2",
    "supertest": "^6.0.0",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
